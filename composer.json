{
    "name": "sl0wik/webset",
    "description": "Set of basic web components.",
    "keywords": ["webset", "content"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Slowik"
        }
    ],
    "require": {
		"php": ">=5.4",
        "illuminate/support": "5.*",
        "sl0wik/laravel-image-editor": "0.*",
        "mcamara/laravel-localization": "^1.3",
        "intervention/image": "^2.4"
    },
	"require-dev": {
		"phpunit/phpunit": "5.2.*"
    },
	"autoload": {
        "psr-4": {
            "Sl0wik\\Webset\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sl0wik\\Webset\\WebsetServiceProvider"
            ],
            "aliases": {
                "Menu": "Sl0wik\\Webset\\WebsetFacade"
            }
        }
    }
}
